{
  "name": "ride-my-way",
  "version": "1.0.0",
  "description": "A carpooling application that provides drivers with the ability to create ride offers and passengers to join available ride offers.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix",
    "start": "node dist/index.js",
    "dev": "nodemon index.js --exec babel-node",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --exit --require babel-core/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "rimraf dist/ && babel ./ -d dist/ --ignore ./coverage,./node_modules,./.babelrc,./package.json,./package-lock.json,./npm-debug.log"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lexcorp16/ride-my-way.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lexcorp16/ride-my-way/issues"
  },
  "homepage": "https://github.com/lexcorp16/ride-my-way#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0"
  },
  "engines": {
    "node": "8.9.1"
  }
}
